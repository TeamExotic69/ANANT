#include<stdio.h>
void main(){
    for(int i=1; i<=5; i++){
        int a=1;
        for(int j=1; j<=i; j++){
            if(i%2==0){
                printf("%c",a+64);
                a++;
                

            }
            else{
                printf("%d",j);
            }
            
        }
        printf("\n");
    }
}
